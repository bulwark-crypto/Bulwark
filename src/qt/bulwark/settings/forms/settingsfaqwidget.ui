                        <property name="text">
                         <string>What is a Master Node?</string>
                        </property>
                        <property name="wordWrap">
                         <bool>true</bool>
                        </property>
                       </widget>
                      </item>
                      <item>
                       <spacer name="verticalSpacer_90">
                        <property name="orientation">
                         <enum>Qt::Vertical</enum>
                        </property>
